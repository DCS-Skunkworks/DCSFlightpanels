<UserControl x:Class="DCSFlightpanels.CustomControls.StreamDeckPushRotaryCtrl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DCSFlightpanels.CustomControls"
             mc:Ignorable="d" 
             d:DesignHeight="78" d:DesignWidth="40">
    <Grid Background="DimGray" Name ="GridBack">
        <Image Name="CCW" Source="/Images/Streamdeck/arrow_rotate_counterclockwise.png" Stretch="Uniform" Visibility="Hidden"/>
        <Image Name="CW" Source="/Images/Streamdeck/arrow_rotate_clockwise.png" Stretch="Uniform" Visibility="Hidden"/>
        <Image Name="Push" Source="/Images/Streamdeck/button.png" Stretch="Uniform" Visibility="Hidden"/>
    </Grid>
</UserControl>
